{
  "_version": "1.65.0",
  "sap.app": {
    "id": "com.ibs.ibsidealtoolpage",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.16.0",
      "toolsId": "7cec84e7-444f-4e1d-a445-a8b6e86d29b2"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/ideal-sa-application/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "ToolPage-display": {
          "semanticObject": "ToolPage",
          "action": "display",
          "title": "{{flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.131.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.ibs.ibsidealtoolpage.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "pageContainer",
        "clearTarget": true,
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.ibs.ibsidealtoolpage.view",
        "async": true,
        "viewPath": "com.ibs.ibsidealtoolpage.view"
      },
      "routes": [
        {
          "name": "RouteMaster",
          "pattern": "Master:?query:",
          "target": [
            "TargetMaster"
          ]
        },
        {
            "name": "RouteWelcome",
            "pattern": "",
            "target": [
                "TargetWelcome"
            ]
        },
        {
            "name": "RouteHelp",
            "pattern": "help",
            "target": [
                "TargetHelp"
            ]
        },
        {
            "name": "RouteMainContent",
            "pattern": "toolpage/mainApplication",
            "target": [
                "TargetMainContent"
            ]
        },
        {
            "name": "RouteAppContent",
            "pattern": "toolpage/Application",
            "target": [
                "TargetAppContent"
            ]
        },
        {
            "name": "RouteLogo",
            "pattern": "RouteLogoPage",
            "target": [
                "RouteTargetLogo"
            ]
        }
        
      ],
      "targets": {
        "TargetMaster": {
          "id": "Master",
          "name": "Master",
          "clearControlAggregation": false
        },
        "TargetWelcome": {
            "id": "Welcome",
            "name": "Welcome"
        },
        "TargetHelp": {
            "id": "Help",
            "name": "Help"
        },
        "TargetMainContent": {
            "id": "mainContent",
            "name": "mainContent"
        },
        "TargetAppContent": {
            "id": "appContent",
            "name": "appContent"
        },
        "RouteTargetLogo": {
            "id": "Logo",
            "name": "Logo"
        }
      }
    },
    "rootView": {
      "viewName": "com.ibs.ibsidealtoolpage.view.Master",
      "type": "XML",
      "id": "Master"
    }
  }
}
