<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core">
    <Dialog showHeader="false"  >
        <Text text="{PopupModel>/imagePopupUrl}"></Text>
        <footer>
            <OverflowToolbar>
                <ToolbarSpacer />
                <Button
                    text="Close"
                    type="Emphasized"
                    press="OnCloseTextPopup" />
            </OverflowToolbar>
        </footer>
    </Dialog>
</core:FragmentDefinition>

